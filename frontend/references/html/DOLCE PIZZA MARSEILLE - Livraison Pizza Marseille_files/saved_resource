  opacity: 1;
  visibility: visible;
  -webkit-transition: all 0.7s ease-in-out;
  transition: all 0.7s ease-in-out;
  -webkit-transform: translateY(0%);
          transform: translateY(0%);
}
.brand-box:hover img.gray {
  opacity: 0;
  visibility: hidden;
  -webkit-transition: all 0.7s ease-in-out;
  transition: all 0.7s ease-in-out;
  -webkit-transform: translateY(-100%);
          transform: translateY(-100%);
}

