/*------------------- 4.00. Why -------------------*/
/* Why 1 ---------------------------------- */
.img-box1 {
  position: relative;
  text-align: right;
  padding: 50px 33px 0 0;
}
.img-box1 .img1 {
  display: inline-block;
}
.img-box1 .shape1 {
