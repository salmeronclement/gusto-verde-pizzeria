@-webkit-keyframes fireAni {
  0% {
    -webkit-transform: scaleY(1.1);
            transform: scaleY(1.1);
  }
  50% {
    -webkit-transform: scaleY(0.7);
            transform: scaleY(0.7);
  }
  100% {
    -webkit-transform: scaleY(1.1);
            transform: scaleY(1.1);
  }
}

@keyframes fireAni {
  0% {
    -webkit-transform: scaleY(1.1);
            transform: scaleY(1.1);
  }
  50% {
    -webkit-transform: scaleY(0.7);
            transform: scaleY(0.7);
  }
  100% {
